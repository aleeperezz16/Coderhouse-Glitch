{{#if obj.length}}
  <table class="table table-dark">
    <thead>
    <tr>
      <th>Nombre</th>
      <th>Precio</th>
      <th>Foto</th>
    </tr>
    </thead>
    <tbody>
    {{#each obj}}
      <tr>
        <td>{{this.title}}</td>
        <td>${{this.price}}</td>
        <td><img src={{this.thumbnail}} width="32"/></td>
      </tr>
    {{/each}}
    </tbody>
  </table>
{{/if}}
{{#unless obj.length}}
  <div class="alert alert-warning" role="alert">
    <h4>No se encontraron productos</h4>
  </div>
{{/unless}}